{
  "manifest_version": 2,

  "name": "Radio Silence",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click Here!"
  },

  "content_scripts": [
     {
       "matches": ["http://jquery.com/*"],
       "js": ["jquery-3.2.0.js", "content.js"]
     },
     {
       "matches": ["*://*/*"],
       "js": ["content.js"],
       "run_at": "document_end"
     }
   ]
}
