{
  "manifest_version": 2,

  "name": "Radio Silence",
  "description": "This extension deletes comment sections of certain websites",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click Here!"
  },

  "permissions": [
    "https://www.youtube.com/*"
  ],

  "content_scripts": [
     {
       "matches": ["*://*/*"],
       "js": ["content.js"],
       "run_at": "document_end"
     }
   ]
}
